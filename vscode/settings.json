{
  "workbench.colorTheme": "Base16 Leaf Tomorrow",
  "workbench.colorCustomizations": {
    "statusBar.background": "#1a1f1c",
    "editor.background": "#121312",
    "editorCursor.foreground": "#90b3b0",
    "editorCursor.background": "#221d18"
  },
  "workbench.editor.labelFormat": "medium",
  "terminal.integrated.fontFamily": "Fira Code Light",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.shell.windows": "C:\\Program Files\\PowerShell\\6.0.0-rc\\pwsh.exe",
  "editor.tabSize": 2,
  "editor.fontFamily": "Fira Code",
  "editor.fontSize": 14,
  "editor.fontLigatures": true,
  "editor.rulers": [
    80, 100
  ],
  "editor.acceptSuggestionOnEnter": "off",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.codeLens": false,
  "editor.folding": false,
  "editor.minimap.enabled": false,
  "editor.smoothScrolling": true,
  "diffEditor.renderSideBySide": false,
  "vim.useCtrlKeys": true,
  "vim.leader": "<space>",
  "vim.otherModesKeyBindings": [
    {
      "before": ["leader", "T"],
      "after": ["<C-pageup>"]
    },
    {
      "before": ["leader", "t"],
      "after": ["<C-pagedown>"]
    },
    {
      "before": ["leader", "s"],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.gotoSymbol"
        }
      ]
    },
    {
      "before": ["leader", "S"],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.showAllSymbols"
        }
      ]
    },
    {
      "before": ["leader", "R"],
      "after": [],
      "commands": [
        {
          "command": "editor.action.rename"
        }
      ]
    },
    {
      "before": ["leader", "r"],
      "after": [],
      "commands": [
        {
          "command": "editor.action.referenceSearch.trigger"
        }
      ]
    },
    {
      "before": ["leader", "H"],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.moveEditorToPreviousGroup"
        }
      ]
    },
    {
      "before": ["leader", "L"],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.moveEditorToNextGroup"
        }
      ]
    },
    {
      "before": ["leader", "b"],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.toggleSidebarVisibility"
        }
      ]
    },
    {
      "before": ["leader", "e"],
      "after": [],
      "commands": [
        {
          "command": "editor.action.marker.next"
        }
      ]
    },
    {
      "before": ["leader", "E"],
      "after": [],
      "commands": [
        {
          "command": "editor.action.marker.prev"
        }
      ]
    },
    {
      "before": ["leader", "h"],
      "after": [],
      "commands": [
        {
          "command": "C_Cpp.SwitchHeaderSource",
          "when": "editorTextFocus && editorLangId == 'c'"
        },
        {
          "command": "C_Cpp.SwitchHeaderSource",
          "when": "editorTextFocus && editorLangId == 'cpp'"
        }
      ]
    }
  ],
  "vim.ignorecase": false,
  "vim.smartcase": false,
  "vim.visualstar": true,
  "vsicons.dontShowNewVersionMessage": true,
  "files.trimTrailingWhitespace": true,
  "git.confirmSync": false,
  "powershell.integratedConsole.showOnStartup": false,
  "search.useIgnoreFilesByDefault": true,
  "C_Cpp.intelliSenseEngine": "Default",
  "C_Cpp.formatting": "Disabled",
  "editor.accessibilitySupport": "off",
  "rust-client.channel": "nightly",
  "material-icon-theme.showWelcomeMessage": false,
  "materialTheme.cache.workbench.settings": {
    "themeColours": "Default"
  },
  "julia.executablePath": "C:\\Users\\parke\\AppData\\Local\\Julia-0.6.0\\bin\\julia.exe",
  "workbench.iconTheme": "material-icon-theme",
  "material-icon-theme.showUpdateMessage": false,
  "workbench.panel.location": "bottom",
  "cmake.generator": "Visual Studio 15 2017",
  "cmake.platform": "x64",
  "cmake.toolset": "host=x64",
  "cmake-tools-helper.cmake_download_path": "C:\\Users\\parke\\.vscode\\extensions\\maddouri.cmake-tools-helper-0.1.1\\cmake_download"
}